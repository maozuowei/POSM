<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>jfpay_preposing</display-name>
	<listener>
    	<display-name>applicationListener</display-name>
    	<listener-class>
			com.jfpay.preposing.utils.ApplicationListener
		</listener-class>
  	</listener>
	
	<servlet>
		<servlet-name>JFpay</servlet-name>
		<servlet-class>
			com.jfpay.preposing.action.ClientInfUnifiedAction
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>JFpay</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
</web-app>